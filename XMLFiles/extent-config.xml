            ]]></reportName>
		<scripts> 
      <![CDATA[ 
        $(document).ready(function() { 
           
        }); 
      ]]>
		</scripts> -->

		<!-- custom styles -->
		<styles> 
            <![CDATA[ 
                .report-name { padding-left: 10px; } .report-name > img { float: left;height: 60%;margin-left: 10px;margin-top: 1px;width: auto; } 
            ]]>
		</styles>
	</configuration>
</extentreports>